<?xml version='1.0'?>

<launch>

  <!-- load the controllers configuration -->
  <rosparam command="load" file="$(find sara_launch)/config/control/capra_arm_joint_state_controller.yaml"/>
  <rosparam command="load" file="$(find sara_launch)/config/control/capra_arm_velocity_controller.yaml"/>
  <!-- <rosparam command="load" file="$(find sara_launch)/config/control/sara_gripper_action_controller.yaml"/> -->
  <!-- <rosparam command="load" file="$(find robotiq_85_gazebo)/controller/gripper_controller_robotiq.yaml"/> -->

  <!--    spawn the controllers    -->
  <node name="controller_spawner" pkg="controller_manager"
        type="spawner" respawn="true"
 	output="screen" args="capra_arm_joint_state_controller
 	                      capra_arm_velocity_controller
 	                       --shutdown-timeout 10"/>

</launch>
